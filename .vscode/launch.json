{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: edenredtools oauth2 local-proxy",
            "cwd": "${workspaceFolder}",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/poetry",
            "console": "integratedTerminal",
            "args": [
                "run",
                "edenredtools",
                "oauth2",
                "local-proxy",
                "-url",
                "http://nowhere.edenred.net/oauth/callback",
                "-port",
                "8888"
            ]
        }
    ]
}